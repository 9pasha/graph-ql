{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "linebreak-style": ["error", "windows"],
        "import/prefer-default-export": "off",
        "implicit-arrow-linebreak": "off",
        "no-return-await": "off",
        "no-underscore-dangle": "off"
    }
}
